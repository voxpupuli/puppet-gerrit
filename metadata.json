{
  "name": "puppet-gerrit",
  "version": "1.0.3-rc0",
  "author": "Vox Pupuli",
  "summary": "Web based code review and project management for Git based projects.",
  "license": "MIT",
  "source": "https://github.com/voxpupuli/puppet-gerrit.git",
  "project_page": "https://github.com/voxpupuli/puppet-gerrit",
  "issues_url": "https://github.com/voxpupuli/puppet-gerrit/issues",
  "description": "Gerrit is a web based code review system, facilitating online code reviews for projects using the Git version control system.",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 7.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ]
}
